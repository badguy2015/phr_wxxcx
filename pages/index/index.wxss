  flex-direction: column;
  align-items: center;
}
.container{
  /* margin-top:50%; */
  display: flex;
  flex-direction: column;
  align-items: center;
}
swiper{
  width: 100%;
  height:240rpx;
}

swiper image{
  width:100%;
  height:240rpx;
}

.select-containerx{
  border: 1px solid #405f80;
  background-color: #405f80;
  height:50rpx;
  width:200rpx;
  border-radius: 5px;
  text-align: center;
  margin:8rpx;
}

.button-sp-area{
    margin: 0 auto;
    padding-top: 15px;
    text-align:center;
}
page{
  /* background:url("https://phr.xsphp.com/res/mobile/images/page_background.jpg") repeat-x; */
  background-color:#eee;
}

.container-view{
  width: 600rpx;
  background-color: white;
  border-radius: 10rpx;
  height: 150rpx;
  margin: 50rpx auto;
  display:flex;
  flex-direction: row;
  align-items: center;
}
.container-view-view-left{
  margin: 0;
  padding-left:20rpx;
  height: 100%;
  width: 20%;
  /* border: #333 dashed 1rpx; */
}
.container-view-image{
  width: 110rpx;
  height: 110rpx;
  margin-top: 10px;
}
.container-view-view-right{
  margin: 0;
  padding: 0 20rpx;
  height: 100%;
  width: 80%;
  /* border: #333 dashed 1rpx; */
  display:flex;
  flex-direction: column;
  align-items: left;
}
.container-view-view-right-text-title{
  text-align: left;
  color: #000;
  font-weight: bold;
  height: 70rpx;
  line-height: 100rpx;
  font-size: 32rpx;

}
.container-view-view-right-text-content{
  text-align:left;
  color:#999;
  font-size: 30rpx;
}

